{
  "expo": {
    "name": "question_translation_app",
    "slug": "question-translation-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "ios": {
      "newArchEnabled": false,
      "infoPlist": {
        "NSCameraUsageDescription": "사진 촬영을 위해 카메라 접근 권한이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "사진을 저장하기 위해 사진 라이브러리 접근 권한이 필요합니다."
      }
    },
    "android": {
      "newArchEnabled": false,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}
